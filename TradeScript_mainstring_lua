local Tab = Window:CreateTab("üåê Discord") -- Title, Image
local Section = Tab:CreateSection("Click to copy discord invite link to clipboard!")

local Button = Tab:CreateButton({
    Name = "Button Example",
    Callback = function()
        local textToCopy = "Click to Copy!" -- Replace with the text you want to copy

        -- Copy text to clipboard
        if not setclipboard then
            return -- setclipboard function is not available
        end
        setclipboard(textToCopy)

        -- Display notification
        local Notification = Instance.new("Message")
        Notification.Parent = game.Workspace
        Notification.Text = "The link has been copied to your clipboard."
        wait(3) -- Adjust the duration you want the notification to be displayed
        Notification:Destroy()
    end,
})
